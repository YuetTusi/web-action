{
	"name": "web-action",
	"version": "0.0.1",
	"description": "WebAction",
	"main": "dist/main.js",
	"repository": "https://github.com/YuetTusi/web-action.git",
	"author": "cuiyue <cuiyue.msn@hotmail.com>",
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist/**/*",
		"build": "webpack --config webpack.dev.js",
		"build:prod": "webpack --config webpack.prod.js",
		"prego": "tsc",
		"serve": "webpack serve --config webpack.dev.js",
		"go": "electron .",
		"start": "cross-env NODE_ENV=development concurrently \"yarn run build\" \"yarn run serve\" \"yarn run go\"",
		"prepack": "yarn run build:prod",
		"pack": "electron-builder --dir",
		"predist": "rimraf dist/**/* && tsc && yarn run build:prod",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "4c9e34def3049532",
		"productName": "网络行为查询评估系统",
		"copyright": "Copyright © 2021 北京万盛华通科技有限公司",
		"directories": {
			"buildResources": "static"
		},
		"asar": true,
		"asarUnpack": [
			"**/*",
			"!node_modules/**/*",
			"!dist/src/**/*"
		],
		"files": [
			"dist/**/*",
			"icon.png",
			"package.json"
		],
		"extraResources": [
			{
				"from": "asset",
				"to": "asset"
			}
		],
		"win": {
			"target": [
				"nsis",
				"zip"
			],
			"icon": "icon.png",
			"publisherName": "yuet",
			"artifactName": "${productName}.${ext}",
			"requestedExecutionLevel": "requireAdministrator"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"installerIcon": "installer.ico",
			"perMachine": true
		}
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.5",
		"@types/lodash": "^4.14.175",
		"@types/md5": "^2.3.1",
		"@types/nedb": "^1.8.12",
		"@types/react": "^17.0.24",
		"@types/react-dom": "^17.0.9",
		"@types/styled-components": "^5.1.14",
		"@types/uuid": "^8.3.1",
		"@ztree/ztree_v3": "^3.5.46",
		"antd": "^4.16.13",
		"antd-dayjs-webpack-plugin": "^1.0.6",
		"concurrently": "^6.2.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.3.0",
		"dayjs": "^1.10.7",
		"dva": "^2.6.0-beta.22",
		"dva-immer": "^1.0.0",
		"echarts": "^5.2.1",
		"electron": "^15.3.0",
		"electron-builder": "^22.11.7",
		"expose-loader": "^3.0.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^5.3.2",
		"less": "^4.1.1",
		"less-loader": "^10.0.1",
		"loader-utils": "^2.0.0",
		"nedb": "^1.8.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"stickpackage": "^3.1.5",
		"style-loader": "^3.3.0",
		"styled-components": "^5.3.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.3",
		"webpack": "^5.54.0",
		"webpack-cli": "^4.8.0",
		"webpack-dev-server": "^4.3.0",
		"yaml-loader": "^0.6.0"
	},
	"dependencies": {
		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21",
		"md5": "^2.3.0",
		"uuid": "^8.3.2",
		"winston": "^3.3.3"
	}
}
